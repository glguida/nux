HAL_MACHINE=@MACHINE@

ifeq ($(HAL_MACHINE),i386)
ARCH_DIR=i386
endif

ifeq ($(HAL_MACHINE),amd64)
ARCH_DIR=amd64
endif

ifeq ($(HAL_MACHINE),riscv64)
ARCH_DIR=riscv64
endif

NOINST=y
PROGRAM=exuser

SRCS+= main.c
SRCS+= $(ARCH_DIR)/crt0.S

@COMPILE_LIBNUXUSER@
@LINK_LIBNUXUSER@
@COMPILE_LIBEC@
@LINK_LIBEC@
